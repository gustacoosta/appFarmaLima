{
    "expo": {
      "name": "Farma Lima",
      "slug": "farmalima",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "userInterfaceStyle": "light",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "loadingIndicator": {
          "image": "https://loading.io/asset/727618",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
        "expo-font"
      ]
    }
  }
  